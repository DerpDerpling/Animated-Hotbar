{
	"schemaVersion": 1,
	"id": "animated-hotbar",
	"version": "${version}",
	"name": "Animated Hotbar",
	"description": "Adds item animations to the hotbar!",
	"authors": [
		"DerpDerpling"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/animated-hotbar/icon.png",
	"environment": "client",
	"entrypoints": {
		"main": [
			"derp.animatedhotbar.AnimatedHotbar"
		],
		"client": [
			"derp.animatedhotbar.AnimatedHotbarClient"
		],
		"fabric-datagen": [
			"derp.animatedhotbar.AnimatedHotbarDataGenerator"
		]
	},
	"mixins": [
		"animated-hotbar.mixins.json",
		{
			"config": "animated-hotbar.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": ">=1.21-",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}